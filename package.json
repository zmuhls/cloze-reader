{
  "name": "cloze",
  "version": "1.0.0",
  "description": "Gutenberg-based cloze deletion practice app built with TypeScript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zmuhls/cloze-reader.git"
  },
  "homepage": "https://zmuhls.github.io/cloze-reader",
  "scripts": {
    "dev": "parcel index.html --port 1234",
    "build": "parcel build index.html --public-url ./",
    "predeploy": "rm -rf dist && npm run build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "fetch-data": "node scripts/fetch-gutenberg-data.js",
    "start": "npm run dev"
  },
  "keywords": [
    "typescript",
    "tailwind",
    "parcel",
    "game"
  ],
  "author": "Zach Muhlbauer",
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-default": "^2.15.2",
    "@parcel/transformer-typescript-tsc": "^2.15.1",
    "@preact/compat": "^18.3.1",
    "@preact/signals": "^2.0.4",
    "@tailwindcss/typography": "^0.5.13",
    "@types/jest": "^29.5.14",
    "autoprefixer": "^10.4.19",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "parcel": "^2.15.2",
    "postcss": "^8.4.38",
    "preact": "^10.26.6",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.3.4",
    "typescript": "^5.4.5"
  }
}
